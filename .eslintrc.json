{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "no-underscore-dangle": "off",
        "camelcase": "off",
        "indent":"off",
        "object-curly-newline": "off",
        "no-trailing-spaces": "off",
        "padded-blocks": "off",
        "newline-per-chained-call": "off",
        "no-else-return": "off"
    }
}
